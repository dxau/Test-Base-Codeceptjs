{
  "root":true,
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "plugins": [
    "security-node"
  ],
  "extends": [
    "google",
    "plugin:security-node/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "newIsCapExceptions": 0,
    "new-cap": 0
  }
}